{
  "name": "norish",
  "version": "0.16.1-beta",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "dev": "tsx watch server.ts",
    "build": "next build && pnpm run build:server && pnpm run update-sw",
    "build:server": "tsdown",
    "update-sw": "node scripts/update-sw-version.js",
    "clean:install": "pnpm install --frozen-lockfile",
    "build:prod": "pnpm run clean:install && cross-env NODE_ENV=production pnpm run build",
    "db:ensure": "tsx db/scripts/ensure-db.ts",
    "db:push": "pnpm run db:ensure && drizzle-kit push --config ./server/db/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config ./server/db/drizzle.config.ts",
    "migrate": "pnpm run db:ensure && drizzle-kit push --config ./server/db/drizzle.config.ts",
    "docker:build": "docker build -f docker/Dockerfile -t norishapp/norish:test .",
    "docker:up": "docker-compose -f docker/docker-compose.yml up",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:tag": "docker tag norishapp/norish:test norishapp/norish:${npm_package_version}",
    "docker:push": "docker push norishapp/norish:${npm_package_version}",
    "docker:push:test": "docker push norishapp/norish:test",
    "start": "node dist-server/server.cjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "i18n:check": "node scripts/check-locale-keys.js"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.39",
    "@ai-sdk/azure": "^3.0.27",
    "@ai-sdk/deepseek": "^2.0.18",
    "@ai-sdk/google": "^3.0.22",
    "@ai-sdk/groq": "^3.0.22",
    "@ai-sdk/mistral": "^3.0.19",
    "@ai-sdk/openai": "^3.0.26",
    "@ai-sdk/openai-compatible": "^2.0.28",
    "@ai-sdk/perplexity": "^3.0.18",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "@trpc/client": "^11.9.0",
    "@trpc/server": "^11.9.0",
    "@trpc/tanstack-react-query": "^11.9.0",
    "@types/uuid": "^11.0.0",
    "ai": "^6.0.77",
    "better-auth": "^1.4.18",
    "bullmq": "^5.67.3",
    "cheerio": "1.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "17.2.4",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "fraction.js": "^5.3.4",
    "fuse.js": "^7.1.0",
    "heic-convert": "^2.1.0",
    "html-entities": "^2.6.0",
    "ioredis": "^5.9.2",
    "jszip": "3.10.1",
    "jsonrepair": "3.13.2",
    "microdata-node": "2.0.0",
    "mime": "4.1.0",
    "next": "16.1.6",
    "next-intl": "^4.8.2",
    "openai": "6.18.0",
    "ollama-ai-provider-v2": "^3.0.0",
    "parse-ingredient": "2.0.1",
    "pg": "8.18.0",
    "pino": "^10.3.0",
    "playwright-core": "npm:rebrowser-playwright-core@^1.52.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "server-only": "^0.0.1",
    "sharp": "^0.34.5",
    "superjson": "^2.2.6",
    "tsdav": "^2.1.7",
    "use-sound": "^5.0.0",
    "usehooks-ts": "^3.1.1",
    "ws": "8.19.0",
    "yt-dlp-wrap": "^2.3.12",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@heroicons/react": "2.2.0",
    "@heroui/react": "^2.8.8",
    "@iconify/react": "^6.0.2",
